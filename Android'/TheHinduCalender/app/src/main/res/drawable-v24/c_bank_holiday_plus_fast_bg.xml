<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="120"
    android:pivotX="50%"
    android:pivotY="50%">
    <item>
        <shape
            android:angle="0"
            android:innerRadiusRatio="2.2"
            android:shape="ring"
            android:thicknessRatio="60.0"
            android:useLevel="false">
            <solid android:color="@color/ring_color_for_bank_holiday_in_calender" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape
            android:angle="0"
            android:innerRadiusRatio="2.2"
            android:shape="ring"
            android:thicknessRatio="60.0"
            android:useLevel="false">
            <solid android:color="@color/solid_color_for_fast_in_calender" />
        </shape>
    </item>
</layer-list>